<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CtrlCentral</servlet-name>
        <servlet-class>Controleur.CtrlCentral</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Saisir</servlet-name>
        <jsp-file>/Jsp/Saisir.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Accueil</servlet-name>
        <jsp-file>/Jsp/Accueil.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Lire</servlet-name>
        <jsp-file>/Jsp/Lire.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>CtrlEnregistrer</servlet-name>
        <servlet-class>Controleur.CtrlEnregistrer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Supprimer</servlet-name>
        <jsp-file>/Jsp/Supprimer.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>CtrlSupprimer</servlet-name>
        <servlet-class>Controleur.CtrlSupprimer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmationSupp</servlet-name>
        <jsp-file>/Jsp/ConfirmationSupp.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>CtrlConfirmation</servlet-name>
        <servlet-class>Controleur.CtrlConfirmation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CtrlModifier</servlet-name>
        <servlet-class>Controleur.CtrlModifier</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modifier</servlet-name>
        <jsp-file>/Jsp/Modifier.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>pageModification</servlet-name>
        <jsp-file>/Jsp/pageModification.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>CtrlCentral</servlet-name>
        <url-pattern>/CtrlCentral</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Saisir</servlet-name>
        <url-pattern>/Saisir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Accueil</servlet-name>
        <url-pattern>/Accueil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Lire</servlet-name>
        <url-pattern>/Lecture</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CtrlEnregistrer</servlet-name>
        <url-pattern>/CtrlEnregistrer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Supprimer</servlet-name>
        <url-pattern>/Supprimer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CtrlSupprimer</servlet-name>
        <url-pattern>/CtrlSupprimer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationSupp</servlet-name>
        <url-pattern>/ConfirmationSupp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CtrlConfirmation</servlet-name>
        <url-pattern>/CtrlConfirmation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CtrlModifier</servlet-name>
        <url-pattern>/CtrlModifier</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modifier</servlet-name>
        <url-pattern>/Modifier</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pageModification</servlet-name>
        <url-pattern>/pageModification</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
